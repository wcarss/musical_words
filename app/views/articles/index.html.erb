<h1>All Your Articles</h1>
<%= link_to 'New Article', new_article_path %>
<table>
  <tr>
    <th>Title:</th>
    <th>Text:</th>
  </tr>

<% @articles.each do |article| %>
  <tr>
    <td><%= link_to article.title, article_path(article.id) %></td>
    <td><%= article.text %></td>
  </tr>
<% end %>
</table>
